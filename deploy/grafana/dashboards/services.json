            "drawStyle": "line",
            "fillOpacity": 0,
            "lineInterpolation": "smooth",
            "lineWidth": 1,
            "showPoints": "auto",
            "spanNulls": true
          }
        },
        "overrides": []
      },
      "gridPos": { "h": 13, "w": 12, "x": 12, "y": 12 },
      "id": 2,
      "options": {
        "legend": { "displayMode": "list", "placement": "bottom", "showLegend": true },
        "tooltip": { "mode": "multi", "sort": "none" }
      },
      "pluginVersion": "11.3.0",
      "targets": [
        {
          "datasource": { "type": "loki", "uid": "${datasource}" },
          "editorMode": "code",
          "expr": "sum by (code) ( count_over_time({service=~\"gate\", level=\"error\"} | json code=\"context.code\" [1m]) )",
          "queryType": "range",
          "refId": "A"
        }
      ],
      "title": "Gate Errors by error code",
      "transparent": true,
      "type": "timeseries"
    }
